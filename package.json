{
  "name": "polaris-starter",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for polaris-starter goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "build:production": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "start": "vite",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "vite build --mode test && ember test --path dist",
    "lint:types": "glint"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/compat": "^4.0.0-alpha.14",
    "@embroider/config-meta-loader": "^1.0.0-alpha.3",
    "@embroider/core": "^4.0.0-alpha.9",
    "@embroider/vite": "^1.0.0-alpha.11",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "1.4.0",
    "@glint/environment-ember-loose": "1.4.0",
    "@glint/environment-ember-template-imports": "^1.4.0",
    "@glint/template": "^1.4.0",
    "@nullvoxpopuli/eslint-configs": "^4.2.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/ember": "^3.0.8",
    "@types/qunit": "^2.19.10",
    "@types/rsvp": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.4",
    "ember-cli": "~5.11.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-load-initializers": "^3.0.0",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-source": "~5.11.0",
    "ember-template-lint": "^6.0.0",
    "ember-truth-helpers": "^4.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.2.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.2",
    "prettier": "^3.3.3",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "qunit": "^2.22.0",
    "qunit-dom": "^3.2.0",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "tracked-built-ins": "^3.1.1",
    "typescript": "^5.5.4",
    "vite": "^6.0.0"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">= 20.x"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "decorator-transforms": "^2.3.0",
    "ember-modifier": "^4.2.0",
    "ember-resources": "^7.0.2",
    "ember-route-template": "^1.0.3",
    "tracked-built-ins": "^3.3.0",
    "tracked-toolbox": "^2.0.0"
  },
  "volta": {
    "node": "20.16.0",
    "pnpm": "9.9.0"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}